<%= form_with url: path, model: model, method: method_type,local: true do |f|%>
  <h2><%= title %></h2>
  <%= f.label :image %>
  <%= f.text_field :image%><br>
  <%= f.label :name %>
  <%= f.text_field :name, required: true %><br>
  <%= f.label :description %>
  <%= f.text_field :description%><br>
  <%= f.label :approximate_age %>
  <%= f.text_field :approximate_age%><br>
  <%= f.label :sex %>
  <%= f.text_field :sex%><br>
  <%= f.submit button_text %>
<% end %>
